{
  "name": "@amiceli/papierjs",
  "version": "0.4.0",
  "description": "Papercss web components",
  "main": "dist/index.cjs.js",
  "module": "dist/index.js",
  "types": "dist/types/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/papierjs/papierjs.esm.js",
  "keywords": [
    "stencil",
    "papercss"
  ],
  "homepage": "https://github.com/amiceli/papierjs",
  "bugs": {
    "url": "https://github.com/amiceli/papierjs/issues"
  },
  "exports": {
    ".": {
      "import": "./dist/papierjs/papierjs.esm.js",
      "require": "./dist/papierjs/papierjs.cjs.js"
    },
    "./my-component": {
      "import": "./dist/components/my-component.js",
      "types": "./dist/components/my-component.d.ts"
    },
    "./loader": {
      "import": "./loader/index.js",
      "require": "./loader/index.cjs",
      "types": "./loader/index.d.ts"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/amiceli/papierjs.git"
  },
  "files": [
    "dist/",
    "loader/"
  ],
  "scripts": {
    "build": "stencil build",
    "build:watch": "stencil build --watch",
    "start": "stencil build --dev --watch --serve",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "generate": "stencil generate",
    "storybook": "storybook dev -p 6006 --no-open",
    "build-storybook": "storybook build",
    "lint": "biome check ."
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@chromatic-com/storybook": "^3.2.4",
    "@commitlint/cli": "^19.7.1",
    "@commitlint/config-conventional": "^19.7.1",
    "@stencil/core": "^4.26.0",
    "@stencil/sass": "^3.0.12",
    "@storybook/addon-controls": "^8.5.8",
    "@storybook/addon-essentials": "^8.5.8",
    "@storybook/addon-interactions": "^8.5.8",
    "@storybook/blocks": "^8.5.8",
    "@storybook/html": "^8.5.8",
    "@storybook/html-vite": "^8.5.8",
    "@storybook/test": "^8.5.8",
    "@types/jest": "^29.5.14",
    "@types/node": "^16.18.11",
    "@types/react": "^19.0.8",
    "dotenv": "^16.4.7",
    "jest": "^29.7.0",
    "jest-cli": "^29.7.0",
    "lefthook": "^1.10.10",
    "storybook": "^8.5.8"
  },
  "optionalDependencies": {
    "puppeteer": "^24.2.1"
  },
  "license": "MIT",
  "dependencies": {
    "feather-icons": "^4.29.2",
    "papercss": "^1.9.2"
  }
}
