{
  "name": "@amiceli/papierjs",
  "version": "0.12.0",
  "description": "Papercss web components",
  "main": "dist/index.cjs.js",
  "module": "dist/index.js",
  "types": "dist/types/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/papierjs/papierjs.esm.js",
  "keywords": [
    "stencil",
    "papercss"
  ],
  "homepage": "https://github.com/amiceli/papierjs",
  "bugs": {
    "url": "https://github.com/amiceli/papierjs/issues"
  },
  "exports": {
    ".": {
      "import": "./dist/papierjs/papierjs.esm.js",
      "require": "./dist/papierjs/papierjs.cjs.js"
    },
    "./my-component": {
      "import": "./dist/components/my-component.js",
      "types": "./dist/components/my-component.d.ts"
    },
    "./loader": {
      "import": "./loader/index.js",
      "require": "./loader/index.cjs",
      "types": "./loader/index.d.ts"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/amiceli/papierjs.git"
  },
  "files": [
    "dist/",
    "loader/"
  ],
  "scripts": {
    "build:icon": "node scripts/copy-svg.js",
    "build": "npm run build:icon && stencil build --doc",
    "build:watch": "npm run build:icon && stencil build --watch",
    "start": "stencil build --dev --watch --serve",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "generate": "stencil generate",
    "lint": "biome check ."
  },
  "overrides": {
    "@types/node": "*"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.14",
    "@commitlint/cli": "^20.4.1",
    "@commitlint/config-conventional": "^20.4.1",
    "@histoire/plugin-vue": "^1.0.0-beta.1",
    "@stencil/core": "^4.26.0",
    "@stencil/sass": "^3.0.12",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.16.5",
    "@vitejs/plugin-vue": "^6.0.4",
    "dotenv": "^16.4.7",
    "histoire": "^1.0.0-beta.1",
    "jest": "^29.7.0",
    "jest-cli": "^29.7.0",
    "lefthook": "^2.1.0",
    "stencil-inline-svg": "^1.1.0"
  },
  "optionalDependencies": {
    "puppeteer": "^24.2.1"
  },
  "license": "MIT",
  "dependencies": {
    "feather-icons": "^4.29.2",
    "markstream-vue": "^0.0.7-beta.8",
    "papercss": "^1.9.2",
    "pixelarticons": "^1.8.1"
  }
}
